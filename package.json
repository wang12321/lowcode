{
  "name": "ff-lowcode",
  "version": "1.0.5",
  "description": "lowcode",
  "author": "FF",
  "bin": {
    "ffLow": "./bin/low"
  },
  "scripts": {
    "mock": "cd mock && node --experimental-modules ./mock.mjs",
    "dev:mock": "concurrently \"vue-cli-service serve\" \"npm run mock\"",
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "lint:style": "stylelint \"src/**/*.(vue|scss|css)\"",
    "fix:style": "stylelint --fix \"src/**/*.(vue|scss|css)\"",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "@smallwei/avue": "^2.8.26",
    "@sscfaith/avue-form-design": "^1.4.8",
    "awe-dnd": "^0.3.4",
    "axios": "0.18.1",
    "commander": "^9.0.0",
    "concurrently": "^6.2.0",
    "core-js": "3.6.5",
    "element-ui": "2.13.2",
    "eslint-plugin-sonarjs": "^0.10.0",
    "ff-auto-router": "^1.1.0",
    "ff-koa": "^1.0.0",
    "fuse.js": "^6.4.6",
    "js-cookie": "2.2.0",
    "js-utils-lc": "^1.0.4",
    "koa-static": "^5.0.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "prettier": "^2.5.1",
    "screenfull": "4.2.0",
    "vue": "2.6.10",
    "vue-ele-component-zonst": "^1.3.0",
    "vue-json-viewer": "^2.2.21",
    "vue-router": "3.0.6",
    "vuedraggable": "^2.24.3",
    "vuex": "3.1.0",
    "runjs": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~11.0.0",
    "@commitlint/config-conventional": "~11.0.0",
    "@vue/cli-plugin-babel": "4.4.4",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-plugin-unit-jest": "4.4.4",
    "@vue/cli-service": "4.4.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "2.4.2",
    "connect": "3.6.6",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "~4.2.5",
    "lint-staged": "~10.5.4",

    "sass": "1.26.8",
    "sass-loader": "8.0.2",
    "script-ext-html-webpack-plugin": "2.1.3",
    "serve-static": "1.13.2",
    "stylelint": "13.13.1",
    "stylelint-config-airbnb": "0.0.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recess-order": "2.4.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "vue-template-compiler": "2.6.10",
    "vue2-ace-editor": "0.0.15"
  },
  "files": [
    "bin",
    "lib",
    "dist"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint src/**/*.{js,vue}",
      "stylelint \"src/**/*.(vue|scss|css)\"",
      "git add ."
    ]
  }
}
